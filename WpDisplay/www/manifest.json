{
  "name": "WP Display",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "2.3.1",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "icons": {
    "96": "icon96.png",
    "128": "icon128.png",
    "144": "icon144.png",
    "192": "icon192.png"
  },
  "app": {
    "background": {
      "scripts": ["launch.js"]
    }
  },
  "permissions": [
    {"socket": [
      "tcp-connect:*:*"
    ]},
    "storage"
  ]
}
